entity ifProcess is
	port(	switch: in bit_vector(3 downto 0); --switch input.
			LED:	out bit_vector(3 downto 0); --LED output.
			
end entity;
architecture ifProcess_arch of ifProcess is
begin
ifProc : process(switch)
	begin
		if(switch = "11111100") then
			LED <= "0000";
			elsif(switch = "01100000") then
			LED <= "0001";
			elsif (switch = "11011010") then
			LED <= "0010";
			elsif (switch = "11110010" ) then
			LED <= "0011";
	elsif (switch = "01100110" ) then
			LED <= "0100";  
	elsif (switch = "10110110" ) then
			LED <= "0101";  
	elsif (switch = "10111110") then
			LED <= "0110";  
	elsif (switch = "11100000" ) then
			LED <= "0111";  	
	elsif (switch = "11111110" ) then
			LED <= "1000";  
	elsif (switch = "11110110" ) then
			LED <= "1001";  
	elsif (switch = "11101110" ) then
			LED <= "1010";  
	elsif (switch = "00111110" ) then
			LED <= "1011";  	
	elsif (switch = "00011010" ) then
			LED <= "1100";  
	elsif (switch = "01111010" ) then
			LED <= "1101"; 
	elsif (switch = "10011110" ) then
			LED <= "1110";  
	else 
			LED <= "1111";
	end if;
end process ifProc;
end architecture ifProcess_arch;